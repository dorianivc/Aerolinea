	<queryString language="SQL">
		<![CDATA[SELECT
     Usuario.`usuario` AS Usuario_usuario,
     Usuario.`contrasena` AS Usuario_contrasena,
     Usuario.`nombre` AS Usuario_nombre,
     Usuario.`apellidos` AS Usuario_apellidos,
     Usuario.`correo_electronico` AS Usuario_correo_electronico,
     Usuario.`fecha_nacimiento` AS Usuario_fecha_nacimiento,
     Usuario.`direccion` AS Usuario_direccion,
     Usuario.`telefono` AS Usuario_telefono,
     Usuario.`celular` AS Usuario_celular,
     Usuario.`admin` AS Usuario_admin,
     Reserva.`reserva` AS Reserva_reserva,
     Reserva.`viaje` AS Reserva_viaje,
     Reserva.`numero_asiento` AS Reserva_numero_asiento,
     Reserva.`pago` AS Reserva_pago,
     Reserva.`usuario` AS Reserva_usuario,
     Viaje.`viaje` AS Viaje_viaje,
     Viaje.`vuelo` AS Viaje_vuelo,
     Viaje.`fecha` AS Viaje_fecha,
     Viaje.`precio` AS Viaje_precio,
     Vuelo.`vuelo` AS Vuelo_vuelo,
     Vuelo.`horario` AS Vuelo_horario,
     Vuelo.`avion_asignado` AS Vuelo_avion_asignado,
     Vuelo.`ruta_asignada` AS Vuelo_ruta_asignada
FROM
     `Usuario` Usuario INNER JOIN `Reserva` Reserva ON Usuario.`usuario` = Reserva.`usuario`
     INNER JOIN `Viaje` Viaje ON Reserva.`viaje` = Viaje.`viaje`
     INNER JOIN `Vuelo` Vuelo ON Viaje.`vuelo` = Vuelo.`vuelo`]]>
